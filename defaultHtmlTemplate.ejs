<html>
<body>
<h1>Default Template</h1>
<% if (Object.keys(metadata).length > 0) { %>
<table>
    <tr>
        <th>Property</th>
        <th>Value</th>
    </tr>
    <% for (const prop in metadata) { %>
        <tr>
            <td><%= prop %></td>
            <td><%= metadata[prop] %></td>
        </tr>
    <% } %>
</table>
<hr>
<% } %>
<%= content %>
</body>
</html>